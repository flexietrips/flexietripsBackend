{
  "kind": "collectionType",
  "collectionName": "tour_destinations",
  "info": {
    "singularName": "tour-destination",
    "pluralName": "tour-destinations",
    "displayName": "tourDestination"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "price": {
      "type": "decimal"
    },
    "destination": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::destination.destination",
      "inversedBy": "tour_destinations"
    },
    "tour_image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
